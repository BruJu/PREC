PREFIX prec: <http://bruy.at/prec#>
PREFIX pgo:  <http://ii.uwb.edu.pl/pgo#>

# Property Graph state

prec:MetaProperty   prec:flagState false .
prec:KeepProvenance prec:flagState false .


# Unconditional property IRI

<https://schema.org/givenName> prec:propertyIRI "givenName" .


# Property IRI different depending on the node label
<https://schema.org/familyName> prec:propertyIRI [
    prec:propertyName "name" ;
    prec:nodeLabel    "PERSON"
] .

<http://example.org/countryName> prec:propertyIRI [
    prec:propertyName "name" ;
    prec:nodeLabel    "COUNTRY"
] .


# Node labels

<http://dbpedia.org/ontology/Person>  prec:nodeLabelIRI "PERSON"  .
<http://dbpedia.org/ontology/Country> prec:nodeLabelIRI "COUNTRY" .

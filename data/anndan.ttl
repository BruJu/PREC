PREFIX prec: <http://bruy.at/prec#>
PREFIX ex:   <http://example.org/>


prec:MetaProperty          prec:flagState false .
prec:KeepProvenance        prec:flagState false .

# (default)
# prec:Relationships prec:useRdfStar prec:RDFReification .

prec:Relationships prec:useRdfStar prec:AsOccurrences .

<http://dbpedia.org/ontology/Person> prec:nodeLabelIRIOf "Person" .

# <https://example.org/likes> prec:relationshipIRIOf "Likes" .

<https://example.org/likes> prec:relationshipIRIOf [
    prec:sourceLabel       "Person" ;
    prec:relationshipLabel "Likes"  ;
    prec:useRdfStar false ;
    prec:subject ex:likedBy
] .

<https://schema.org/name> prec:propertyIRIOf "name" .

PREFIX prec: <http://bruy.at/prec#>
PREFIX ex:   <http://example.org/>


prec:MetaProperty          prec:flagState false .
prec:KeepProvenance        prec:flagState false .

# (default)
# prec:Relationships prec:useRdfStar prec:RDFReification .

prec:Relationships prec:useRdfStar prec:AsOccurrences .

<http://dbpedia.org/ontology/Person> prec:nodeLabelIRI "Person" .

# <https://example.org/likes> prec:relationshipIRI "Likes" .

<https://example.org/likes> prec:relationshipIRI [
    prec:labelName "Likes" ;
    prec:useRdfStar false ;
    prec:subject [ prec:nodeLabel "Person" ; prec:rename ex:likedBy ]
] .

<https://schema.org/name> prec:propertyIRI "name" .
